    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="numA, numB, numC" type="Integer" array="False" size=""/>
            <input variable="numA"/>
            <input variable="numB"/>
            <input variable="numC"/>
            <if expression="numA+numB">
                <then>
                    <if expression="numB=numC">
                        <then>
                            <output expression="&quot;esse triangulo e acutangulo pois os todos os seus angulos s&#227;o menores que 90 graus&quot;" newline="True"/>
                        </then>
                        <else>
                            <output expression="&quot;esse triangulo e retangulo pois um dos seus angulos forma 90 graus&quot;" newline="True"/>
                        </else>
                    </if>
                </then>
                <else>
                    <if expression="numB=numC">
                        <then/>
                        <else>
                            <output expression="&quot;esse triangulo e obtusangulo pois os angulos internos s&#227;o maiores que 90 graus&quot;" newline="True"/>
                        </else>
                    </if>
                </else>
            </if>
            <if expression="(numA+numB)&gt;numC">
                <then>
                    <output expression="&quot;&#233; possivel formar um triangulo&quot;" newline="True"/>
                </then>
                <else>
                    <output expression="&quot;n&#227;o &#233; possivel formar um triangulo&quot;" newline="True"/>
                </else>
            </if>
        </body>
    </function>
